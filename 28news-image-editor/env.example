# Environment Configuration
NODE_ENV=development
PORT=3001

# Backend URLs
BACKEND_BASE_URL=http://localhost:3000
NEXT_PUBLIC_BACKEND_BASE_URL=http://localhost:3000

# BullMQ Queue Configuration
# This suffix will be appended to queue names to separate environments
# Examples: 
# - development: "image-processing-development", "batch-jobs-development"
# - production: "image-processing-production", "batch-jobs-production"
BULLMQ_QUEUE_SUFFIX=development

# For production, use:
# BULLMQ_QUEUE_SUFFIX=production
